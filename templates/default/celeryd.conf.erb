CELERYD_CHDIR=<%= @chdir %>
ENV_PYTHON=$CELERYD_CHDIR/virtualenv/bin/python
<% if @newrelic %>
CELERYD_MULTI="NEWRELIC_CONFIG_FILE=<%= @chdir $>/newrelic.ini newrelic-admin run-program $ENV_PYTHON $CELERYD_CHDIR/manage.py deployed celery multi"
<% else %>
CELERYD_MULTI="$ENV_PYTHON $CELERYD_CHDIR/manage.py deployed celery multi"
<% end %>
